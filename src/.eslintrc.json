{
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "uber-es2015",
    "eslint-config-uber-es2015",
    "eslint-config-uber-jsx"
  ],
  "rules": {
    "consistent-return": 0,
    "max-params": ["error", 6],
    "object-curly-spacing": 0,
    "babel/object-curly-spacing": 2,
    "curly": 0,
    "radix": 0,
    "no-else-return": 0,
    "no-unreachable": "error",
    "eqeqeq": ["error", "always"],
    "prefer-const": ["error", {
      "destructuring": "any",
      "ignoreReadBeforeAssign": false
  }]
  },
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "babel"
  ]
}